{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "encrypted": {
    "gemfuryReadToken": ""
  },
  "python": {
    "registryUrls": [
      "https://${GEMFURY_READ_TOKEN}@pypi.fury.io/habxtech/",
      "https://pypi.org/simple/"
    ]
  },
  "masterIssue": true,
  "rangeStrategy": "bump",
  "prConcurrentLimit": 3,
  "prHourlyLimit": 5,
  "enabledManagers": [
    "pip"
  ],
  "packageRules": [
    {
      "updateTypes": [
        "minor",
        "patch"
      ],
      "depTypeList": [
        "dependencies",
        "devDependencies"
      ],
      "automerge": true,
      "labels": [
        "habx-automerge"
      ]
    }
  ]
}